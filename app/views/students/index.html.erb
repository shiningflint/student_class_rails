<h1>Students List</h1>
<ul>
  <% @students.each do |student| %>
    <li><%= link_to student.name, student_path(student) %></li>
  <% end %>
</ul>

<p><%= link_to "Register new student", new_student_path, class: "button button-primary" %></p>
<p><%= link_to "Back to top", root_path, class: "button" %></p>
